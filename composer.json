{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "cxycdz/curl": "^1.0",
        "latrell/alipay": "dev-master",
        "overtrue/wechat": "~3.1",
        "overtrue/laravel-wechat": "~3.0",
        "simplesoftwareio/simple-qrcode": "~1",
        "doctrine/dbal": "^2.5",
        "zizaco/entrust": "5.2.x-dev",
        "comodojo/zip":"2.1.0",
        "dingo/api": "1.0.*@dev",
        "tymon/jwt-auth":"1.0.0-alpha.3",
        "flc/alidayu": "^2.0",
        "maatwebsite/excel": "~2.1.0",
        "qiniu/php-sdk": "^7.1",
        "jpush/jpush": "v3.5.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Alipayopen\\Sdk\\":"packges/ccmknt/alipayopen/src/sdk/",
            "Alipayopen\\":"packges/ccmknt/alipayopen/src/",
            "Alipayopen\\Sdk\\Request\\":"packges/ccmknt/alipayopen/src/sdk/request/",
            "WeixinPay\\":"packges/ccmknt/weixinpay/src/",
            "WeixinPay\\Lib\\":"packges/ccmknt/weixinpay/src/lib"

        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
